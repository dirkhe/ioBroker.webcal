{
	"i18n": true,
	"type": "tabs",
	"items": {
		"test": {
			"i18n": false,
			"type": "cus__tom",
			"newLine": true,
			"hidden": true,
			"url": "custom/ConfigCustomHooks.js",
			"name": "AdminComponentHooks/Components/ConfigCustomHooks"
		},
		"_Settings": {
			"type": "panel",
			"label": "common",
			"sm": 12,
			"md": 12,
			"lg": 12,
			"items": {
				"daysEventFuture": {
					"type": "number",
					"label": "fetch future days for events",
					"min": 0,
					"max": 30,
					"step": 1,
					"default": 3,
					"newLine": true
				},
				"daysEventPast": {
					"type": "number",
					"label": "fetch days in past for events",
					"min": 0,
					"max": 30,
					"step": 1,
					"width": "50%",
					"default": 0
				},
				"daysJSONFuture": {
					"type": "number",
					"label": "fetch future days for JSON table",
					"hidden": "true",
					"min": 0,
					"max": 30,
					"step": 1,
					"width": "50%",
					"default": 5,
					"newLine": true
				},
				"daysJSONPast": {
					"type": "number",
					"label": "fetch days in past for JSON table",
					"hidden": "true",
					"min": 0,
					"max": 30,
					"step": 1,
					"default": 0,
					"width": "50%"
				},
				"intervall": {
					"type": "number",
					"label": "update intervall in minutes",
					"min": 10,
					"step": 10,
					"width": "50%",
					"default": 60,
					"newLine": true
				}
			}
		},
		"_events": {
			"type": "panel",
			"label": "Events",
			"items": {
				"events": {
					"type": "table",
					"label": "Eventlist",
					"items": [
						{
							"type": "text",
							"attr": "name",
							"title": "name of event",
							"width": "50%",
							"filter": false,
							"sort": false,
							"default": ""
						},
						{
							"type": "text",
							"attr": "regEx",
							"title": "regex search string - will be apply as //i",
							"width": "50%",
							"filter": false,
							"sort": false,
							"default": ""
						}
					]
				}
			}
		},
		"_calendars": {
			"type": "panel",
			"label": "Calendars",
			"items": {
				"_txt": {
					"type": "staticText",
					"text": "test is currently not working, because fix in admin needed, see https://github.com/ioBroker/adapter-react-v5/issues/98"
				},
				"calendars": {
					"type": "table",
					"sm": 12,
					"md": 12,
					"lg": 12,
					"label": "Calendar list",
					"help": "Here you can define your calendar accounts",
					"helpLink": "https://tsdav.vercel.app/docs/intro",
					"items": [
						{
							"type": "checkbox",
							"attr": "inactive",
							"width": "2%",
							"title": "inactive",
							"filter": false,
							"sort": false,
							"default": false
						},
						{
							"type": "text",
							"attr": "name",
							"width": "10%",
							"title": "name",
							"filter": false,
							"sort": true,
							"default": "Calendar 1"
						},
						{
							"type": "select",
							"attr": "authMethod",
							"width": "2%",
							"title": "authorization method",
							"options": [
								{ "label": "Basic", "value": "Basic" },
								{ "label": "Oauth", "value": "Oauth" },
								{ "label": "google", "value": "google" }
							],
							"filter": false,
							"sort": false,
							"default": "Basic"
						},
						{
							"type": "text",
							"attr": "serverUrl",
							"width": "20%",
							"title": "Calendar URL",
							"filter": false,
							"sort": false,
							"default": "https://",
							"hidden": "data.authMethod == 'google'"
						},
						{
							"type": "text",
							"attr": "username",
							"width": "10%",
							"title": "User name",
							"filter": false,
							"sort": false,
							"default": "",
							"hidden": "data.authMethod == 'google'"
						},
						{
							"type": "password",
							"repeat": false,
							"visible": false,
							"attr": "password",
							"width": "10%",
							"title": "Password/ Secret",
							"filter": false,
							"sort": false,
							"default": false
						},
						{
							"type": "text",
							"attr": "tokenUrl",
							"width": "15%",
							"title": "token Url",
							"filter": false,
							"sort": false,
							"default": "https://",
							"hidden": "data.authMethod != 'Oauth'"
						},
						{
							"type": "text",
							"attr": "refreshToken",
							"width": "8%",
							"title": "refreshToken width calDav permission",
							"filter": false,
							"sort": false,
							"default": "",
							"hidden": "data.authMethod == 'Basic'"
						},
						{
							"type": "text",
							"attr": "clientId",
							"width": "10%",
							"title": "clientId",
							"filter": false,
							"sort": false,
							"default": "",
							"hidden": "data.authMethod == 'Basic'"
						},
						{
							"type": "checkbox",
							"attr": "ignoreSSL",
							"width": "2%",
							"title": "Ignore SSL errors",
							"filter": false,
							"sort": false,
							"default": false,
							"hidden": "data.authMethod == 'google'"
						},
						{
							"type": "sendTo",
							"command": "testCalendar",
							"label": "test",
							"jsonData": "{\"name\":\"${data.name}\",\"calData\":{\"authMethod\":\"${data.authMethod}\",\"serverUrl\":\"${data.serverUrl}\",\"tokenUrl\":\"${data.tokenUrl}\",\"refreshToken\": \"${data.refreshToken}\",\"username\": \"${data.username}\",\"clientId\": \"${data.clientId}\",\"password\": \"${data.password}\",\"ignoreSSL\": ${data.ignoreSSL}}}",
							"width": "10%",
							"title": "test settings",
							"showProcess": true
						}
					]
				}
			}
		}
	}
}
